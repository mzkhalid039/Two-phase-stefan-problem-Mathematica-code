      RowBox[{"1", ",", 
       RowBox[{"\[Beta]", "*", "2"}]}], "]"}]}], "-", 
    RowBox[{
     RowBox[{"BesselJ", "[", 
      RowBox[{"1", ",", 
       RowBox[{"\[Beta]", "*", "2"}]}], "]"}], 
